<?xml version="1.0" encoding="UTF-8"?>
<fom>
	<contexts>
		<context class="example.fom.xml.mybatis.InputOracleWithMybatisContext">
			<remark>将指定目录下zip包解析导入Oracle</remark>
			<cron>0/30 * * * * ?</cron>
			<pattern>demo.zip</pattern>
			<srcPath>/source</srcPath>
			<batch>5000</batch>
			<isDelMatchFail>false</isDelMatchFail>
			<zipEntryPattern>.*[.]txt</zipEntryPattern>
		</context>

		<context name="InputOracleWithPoolDemo" class="example.fom.xml.pool.InputOracleWithPoolContext">
			<remark>将指定目录下zip包解析导入Oracle</remark>
			<cron>0/10 * * * * ?</cron>
			<pattern>test.zip</pattern>
			<srcPath>/source</srcPath>
			<batch>5000</batch>
			<isDelMatchFail>false</isDelMatchFail>
			<zipEntryPattern>.*[.]bcp</zipEntryPattern>
		</context>
	</contexts>
</fom>