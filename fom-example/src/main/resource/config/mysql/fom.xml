<?xml version="1.0" encoding="UTF-8"?>
<fom>

<fom-scan>com.examples.b, com.a</fom-scan>

	<contexts>
		<context class="com.examples.task.input.InputMysqlExample1">
			<remark>使用Mybatis的方式将本地指定目录下text文本解析导入Mysql库</remark>
			<cron>0/5 * * * * ?</cron>
			<pattern>demo.txt</pattern>
			<srcPath>/source</srcPath>
			<batch>5000</batch>
			<isDelMatchFail>false</isDelMatchFail>
		</context>

		<context name="InputMysqlExample2" class="com.examples.task.input.InputMysqlExample2">
			<remark>使用自定义pool的方式将本地指定目录下Excel解析导入Mysql库</remark>
			<cron>0/10 * * * * ?</cron>
			<pattern>test.xlsx</pattern>
			<srcPath>/source</srcPath>
			<batch>5000</batch>
			<isDelMatchFail>false</isDelMatchFail>
		</context>
	</contexts>
</fom>