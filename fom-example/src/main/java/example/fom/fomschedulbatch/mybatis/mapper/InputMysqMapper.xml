<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="example.fom.fomschedulbatch.mybatis.mapper.InputMysqMapper">

	<insert id="input" parameterType="List">
		insert into demo(id,name,source,filetype,importway) values
		<foreach collection="list" item="item" index="index" separator=",">
			(#{item.id},#{item.name},#{item.source},#{item.fileType},#{item.importWay})
		</foreach>
	</insert>
	
</mapper>