<?xml version="1.0" encoding="UTF-8"?>
<pools>
	<pool>
		<name>es</name>
		<class>com.fom.pool.EsPool</class>
		<clusterName>esName</clusterName>
		<nodes>
			<node>ip:port</node>
		</nodes>
	</pool>

	<pool>
		<name>oracle</name>
		<core>20</core>
		<max>20</max>
		<waitTimeOut>120000</waitTimeOut>
		<aliveTimeOut>120000</aliveTimeOut>
		<class>com.fom.pool.JdbcPool</class>
		<driver>oracle.jdbc.driver.OracleDriver</driver>
		<url>jdbc:oracle:thin:@ip:port:oracleName</url>
		<user>user</user>
		<passwd>passwd</passwd>
	</pool>
	
	<pool>
		<name>mysql</name>
		<core>20</core>
		<max>20</max>
		<waitTimeOut>120000</waitTimeOut>
		<aliveTimeOut>120000</aliveTimeOut>
		<class>com.fom.pool.JdbcPool</class>
		<driver>com.mysql.cj.jdbc.Driver</driver>
		<url>jdbc:mysql://ip:port/mysqlName</url>
		<user>user</user>
		<passwd>passwd</passwd>
	</pool>
</pools>
