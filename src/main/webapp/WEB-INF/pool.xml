		<aliveTimeOut>300000</aliveTimeOut>
		<class>com.fom.util.db.pool.PoolOracle</class>
		<url>jdbc:oracle:thin:@ip:1521:ora11g</url>
		<user>user</user>
		<passwd>passwd</passwd>
	</pool>
</pools>
