<?xml version="1.0" encoding="UTF-8"?>
<pools>
	<pool>
		<name>demoEs</name>
		<class>com.fom.util.db.pool.PoolEs</class>
		<clusterName>myEs</clusterName>
		<nodes>
			<node>ip:9300</node>
		</nodes>
	</pool>

	<pool>
		<name>demoOracle</name>
		<core>20</core>
		<max>20</max>
