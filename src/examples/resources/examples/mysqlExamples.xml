<?xml version="1.0" encoding="UTF-8"?>
<fom>
	<configs>
		<config name="ImportMysqlExample1" class="com.fom.examples.TextImporterConfig">
		<!--  使用Mybatis的方式将本地指定目录下text文本解析导入Mysql库-->
			<pattern>demo.txt</pattern>
			<src.path>${webapp.root}/source</src.path>
			<importer.batch>5000</importer.batch>
			<importer.isDelMatchFail>false</importer.isDelMatchFail>
		</config>

		<config name="ImportMysqlExample2" class="com.fom.examples.TextImporterConfig">
		<!--使用自定义pool的方式将本地指定目录下text文本解析导入Mysql库  -->
			<pattern>test.txt</pattern>
			<src.path>${webapp.root}/source</src.path>
			<importer.batch>5000</importer.batch>
			<importer.isDelMatchFail>false</importer.isDelMatchFail>
		</config>
	</configs>
</fom>