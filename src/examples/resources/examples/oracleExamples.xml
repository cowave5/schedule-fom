<?xml version="1.0" encoding="UTF-8"?>
<fom>
	<configs>
		<config name="ImportOracleExample1" class="com.fom.examples.TextZipImporterConfig">
		<!--使用Mybatis的方式将本地指定目录下text文本的zip包解析导入Oracle库  -->
			<pattern>demo.zip</pattern>
			<src.path>${webapp.root}/source</src.path>
			<importer.batch>5000</importer.batch>
			<importer.isDelMatchFail>false</importer.isDelMatchFail>
			<zip.entryPattern>.\.bcp</zip.entryPattern>
		</config>

		<config name="ImportOracleExample2" class="com.fom.examples.TextZipImporterConfig">
		<!--使用自定义pool的方式将本地指定目录下text文本的zip包解析导入Oracle库  -->
			<pattern>test.zip</pattern>
			<src.path>${webapp.root}/source</src.path>
			<importer.batch>5000</importer.batch>
			<importer.isDelMatchFail>false</importer.isDelMatchFail>
			<zip.entryPattern>.\.bcp</zip.entryPattern>
		</config>
	</configs>
</fom>