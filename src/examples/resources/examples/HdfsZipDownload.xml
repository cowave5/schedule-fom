<?xml version="1.0" encoding="UTF-8"?>
<fom>
	<configs>
		<config name="HdfsZipDownload" class="com.fom.defaulter.HdfsZipDownloadConfig">
			<src.path>/test</src.path>
			<hdfs.master>master.ip:port</hdfs.master>
			<hdfs.slave>slave.ip:port</hdfs.slave>
			<hdfs.signalFile>a.orc</hdfs.signalFile>
			<downloader.isSrcDel>false</downloader.isSrcDel>
			<downloader.isWithTemp>true</downloader.isWithTemp>
			<downloader.desPath>${webapp.root}/download</downloader.desPath>
			<downloader.zip.entryMax>100</downloader.zip.entryMax>
			<downloader.zip.sizeMax>5M</downloader.zip.sizeMax>
		</config>
	</configs>
</fom>